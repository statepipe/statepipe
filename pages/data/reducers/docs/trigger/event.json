{
    "description" : [
      "Realizam operações no `event` enviado elemento quando um trigger é disparado."
    ], 
    "reducers": [
        {
            "name":"eventFn",
            "description": [
                "Run any function provided on arguments over the *event*."
            ],
            "schemas": [
                "`eventFn()` return state",
                "`eventFn(preventDefault)` event.preventDefault(); return state",
                "`eventFn(foo,bar)` event.foo(); event.bar(); return state"
            ]
        },
        {
            "name":"eventPick",
            "description": [
                "Run ramda's [pick](https://ramdajs.com/docs/#takeLast) over the *event*."
            ],
            "schemas": [
                "`eventPick(keyCode,charCode)` {...state, pick(['keyCode','charCode'], state)}"
            ]
        }
    ]
}
